    "deep-freeze": "0.0.1",
    "express": "^4.16.3",
    "express-rest": "^1.0.4",
    "fs-extra": "^6.0.1",
    "helmet": "^3.18.0",
    "json-stable-stringify": "^1.0.1",
    "json5": "^1.0.1",
    "lodash": "^4.17.14",
    "nconf": "^0.10.0",
    "shoppinglist-shared": "link:../shared",
    "ws": "^5.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
