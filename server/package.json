{
  "name": "shoppinglist-server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "production": "NODE_ENV=production node build/index.js | bunyan",
    "start": "nodemon --watch src --watch ../shared/build --exec babel-node src/index.ts --extensions '.ts' | bunyan",
    "build": "yarn run build:clean && yarn run build:babel",
    "build:clean": "rm -rf ./build",
    "build:babel": "babel src --out-dir build --source-maps --extensions '.ts'",
    "test": "jest src",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "format": "yarn run format:imports; yarn run format:prettier",
    "format:imports": "organize-imports-cli tsconfig.json",
    "format:prettier": "prettier src --write"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "camel-case": "^4.1.1",
    "cors": "^2.8.5",
    "date-fns": "^2.12.0",
    "deep-freeze": "0.0.1",
    "express": "^4.16.3",
    "express-rest": "^1.0.4",
    "fs-extra": "^9.0.0",
    "helmet": "^3.18.0",
    "json-stable-stringify": "^1.0.1",
    "json5": "^2.1.3",
    "lodash": "^4.17.14",
    "nconf": "^0.10.0",
    "shoppinglist-shared": "1.0.0",
    "ws": "^5.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/plugin-transform-typescript": "^7.9.4",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/babel__core": "^7.1.7",
    "@types/babel__plugin-transform-runtime": "^7.9.0",
    "@types/babel__preset-env": "^7.9.0",
    "@types/body-parser": "^1.19.0",
    "@types/bunyan": "^1.8.6",
    "@types/cors": "^2.8.6",
    "@types/deep-freeze": "0.1.2",
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^8.1.0",
    "@types/helmet": "^0.0.45",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/json5": "^0.0.30",
    "@types/lodash": "^4.14.149",
    "@types/nconf": "^0.10.0",
    "@types/nodemon": "^1.19.0",
    "@types/prettier": "2.0.0",
    "@types/ws": "^7.2.3",
    "jest": "^24.9.0",
    "nodemon": "^2.0.3"
  }
}
