{
  "name": "shoppinglist-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "production": "NODE_ENV=production node build/index.js | npx bunyan",
    "start": "npx nodemon --watch src --watch ../shared/build --exec npx babel-node src/index.js | npx bunyan",
    "build": "npm run build:clean && npm run build:babel",
    "build:clean": "rm -rf ./build",
    "build:babel": "npx babel src --out-dir build --source-maps",
    "test": "npx jest --roots src",
    "flow": "npx flow"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "camel-case": "^3.0.0",
    "date-fns": "^1.30.1",
    "deep-freeze": "0.0.1",
    "express": "^4.16.3",
    "express-rest": "^1.0.4",
    "fs-extra": "^6.0.1",
    "helmet": "^3.18.0",
    "json-stable-stringify": "^1.0.1",
    "json5": "^1.0.1",
    "lodash": "^4.17.14",
    "nconf": "^0.10.0",
    "shoppinglist-shared": "link:../shared",
    "ws": "^5.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-flow": "^7.0.0",
    "flow-bin": "^0.102.0",
    "flow-typed": "^2.5.2",
    "jest": "^24.8.0",
    "nodemon": "^1.18.1"
  }
}
